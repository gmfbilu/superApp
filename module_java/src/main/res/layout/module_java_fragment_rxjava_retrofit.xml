<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="MissingDefaultResource">

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/module_java_appBarLayout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:ignore="MissingConstraints">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/module_java_toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/android:actionBarSize"
            android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"/>
    </com.google.android.material.appbar.AppBarLayout>


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/module_java_bt_singleRequest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="单个请求"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/module_java_appBarLayout"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/module_java_bt_multiRequest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="多个请求合并数据"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/module_java_bt_singleRequest"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/module_java_bt_file"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="上传文件并且有进度回调"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/module_java_bt_multiRequest"/>



</androidx.constraintlayout.widget.ConstraintLayout>