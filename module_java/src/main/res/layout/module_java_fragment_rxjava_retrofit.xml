<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="MissingDefaultResource">

    <android.support.design.widget.AppBarLayout
        android:id="@+id/module_java_appBarLayout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:ignore="MissingConstraints">

        <android.support.v7.widget.Toolbar
            android:id="@+id/module_java_toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/android:actionBarSize"
            android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"/>
    </android.support.design.widget.AppBarLayout>


    <android.support.v7.widget.AppCompatButton
        android:id="@+id/module_java_bt_singleRequest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="单个请求"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/module_java_appBarLayout"/>

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/module_java_bt_multiRequest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="多个请求合并数据"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/module_java_bt_singleRequest"/>


</android.support.constraint.ConstraintLayout>